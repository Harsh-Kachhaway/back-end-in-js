{
  "name": "is-webview",
  "version": "1.0.1",
  "description": "JavaScript library to indicate if we are dealing with a WebView or a Web page.",
  "main": "index.js",
  "scripts": {
    "test": "tape test/*.js",
    "build": "browserify -s is-webview -e index.js > public/index.js",
    "deploy": "git subtree push --prefix public origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.prototype0.net:freebird/is-webview.git"
  },
  "keywords": [
    "dom",
    "webview",
    "webapp",
    "sniffing",
    "detection",
    "mobile"
  ],
  "author": "Thomas Parisot <thomas.parisot@bbc.co.uk> (https://oncletom.io)",
  "browser": {
    "./lib/noop.js": "./lib/dom.js"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "chrome/canary",
      "safari/latest",
      "ipad/latest",
      "android/latest"
    ]
  },
  "license": "Apache-2",
  "dependencies": {
    "global": "^4.3.0"
  },
  "devDependencies": {
    "mocha": "^1.21.5",
    "tape": "^3.0.0",
    "testling": "^1.7.1"
  }
}
